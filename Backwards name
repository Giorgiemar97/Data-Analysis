#include <iostream>
#include <string>
using namespace std;

int main()
{
	/*

		in: Sarah
		out: haraS
	*/

	string word = "Sarah"; //5 characters in this name.

    cout << word[4] << endl;
    cout << word[3] << endl;
    cout << word[2] << endl;
    cout << word[1] << endl;
    cout << word[0] << endl;

    for (int i = word.length() - 1; i >= 0; i--)
        cout << word[i];


	return 0;
}
